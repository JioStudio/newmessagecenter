{
    "components": {
        "securitySchemes": {
            "petstore_auth": {
                "type": "oauth2",
                "description": "This API uses OAuth 2 with the client credential grant flow.",
                "flows": {
                    "clientCredentials": {
                        "tokenUrl": "https://localhost:6882/token",
                        "scopes": {
                            "write:pets": "modify pets in your account",
                            "read:pets": "read your pets",
                            "server.info.r": "read server info"
                        }
                    }
                }
            }
        },
        "schemas": {
            "Error": {
                "required": [
                    "code",
                    "message"
                ],
                "properties": {
                    "message": {
                        "type": "string"
                    },
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "Pet": {
                "required": [
                    "id",
                    "name"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "id": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "tag": {
                        "type": "string"
                    }
                }
            }
        }
    },
    "servers": [
        {
            "url": "http://petstore.swagger.io/v1"
        }
    ],
    "openapi": "3.0.0",
    "paths": {
        "/health": {
            "get": {
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "parameters": []
            }
        },
        "/pets/{petId}": {
        },
        "/server/info": {
            "get": {
                "security": [
                    {
                        "petstore_auth": [
                            "server.info.r"
                        ]
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "parameters": []
            }
        },
        
        "/getmessage": {
            "post": {
                "security": [
                    {
                        "petstore_auth": [
                            "server.info.r"
                        ]
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "parameters": []
            }
        },
        "/addmessage": {
        	"post":{}
        },
        "/deletemessage": {
        	"post":{}
        },
        "/extmessageremove":{
        	"post":{}
        },"/extmessageappend":{
        	"post":{
					"requestBody": {
                    	"description": "Pet to add to the store",
                   	 	"required": true,
                    	"content": {
                        	"application/json": {
                            	"schema": {
                                }
                            }
                        }
                    }
		     }
        },"/extmessageupdate":{
        	"post":{}
        },"/extmessageupdateone":{
        	"post":{}
        }
        
    },
    "info": {
        "version": "1.0.0",
        "title": "Swagger Petstore",
        "license": {
            "name": "MIT"
        }
    }
}